{
  "Read input Rust Snippet": {
    "prefix": "snippet_read_input",
    "body": [
      "fn read_input() -> String {",
      "    let mut input_str = String::new();",
      "    std::io::stdin()",
      "        .read_line(&mut input_str)",
      "        .expect(\"Failed to read line\");",
      "    input_str",
      "}"
    ],
    "description": "Read input Rust Snippet"
  },
  "Read Number Into Num": {
    "prefix": "read_number_into_num",
    "body": ["let num = read_input().trim().parse::<${1:type}>().unwrap();"],
    "description": "Read Number Into Num"
  },
  "Read Text Into Vec": {
    "prefix": "read_text_into_vec",
    "body": [
      "let inp_vec = read_input()",
      "    .split_whitespace()",
      "    .map(|x| x.parse::<${1:type}>().unwrap())",
      "    .collect::<Vec<${1:type}>>();"
    ],
    "description": "Read Text Into Vec"
  }
}
